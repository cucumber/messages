<% @schemas.each do |key, schema| -%>
export type <%= class_name(key) -%> = {
  <%- schema['properties'].each do |property_name, property| -%>
    <%- if (schema['required'] || []).index(property_name) -%>
      <%- %>  <%= property_name %>: <%= type_for(class_name(key), property_name, property) -%>
    <%- else -%>
      <%- %>  <%= property_name %>?: <%= type_for(class_name(key), property_name, property) -%>
    <%- end -%>

  <%- end -%>
}

<% end -%>
