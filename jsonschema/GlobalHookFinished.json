{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "GlobalHookFinished.json",
  "additionalProperties": false,
  "required": [
    "testRunStartedId",
    "hookId",
    "result",
    "timestamp"
  ],
  "properties": {
    "testRunStartedId": {
      "description": "Identifier for the test run that this hook execution belongs to",
      "type": "string"
    },
    "hookId": {
      "description": "Identifier for the hook that was executed",
      "type": "string"
    },
    "result": {
      "$ref": "./TestStepResult.json"
    },
    "timestamp": {
      "$ref": "./Timestamp.json"
    }
  },
  "type": "object"
}